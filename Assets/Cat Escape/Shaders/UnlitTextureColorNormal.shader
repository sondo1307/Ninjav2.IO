			float3 ase_vertexNormal = mul( unity_WorldToObject, float4( ase_worldNormal, 0 ) );
			o.Emission = ( ( _Color * tex2D( _MainTex, uv_MainTex ) ) + ( float4( saturate( ( ( mul( UNITY_MATRIX_V, float4( ase_worldNormal , 0.0 ) ).xyz * 0.5 ) + 0.5 ) ) , 0.0 ) * _WorldNormalColor ) + ( float4( saturate( ase_vertexNormal ) , 0.0 ) * _VertexNormalColor ) ).rgb;
			o.Alpha = 1;
		}

